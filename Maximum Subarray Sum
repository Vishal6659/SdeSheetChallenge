#include <bits/stdc++.h> 
long long maxSubarraySum(int arr[], int n)
{
   long long int maxSum = INT_MIN,  sum = 0;
       
    for(int i = 0; i < n; i++)
    {
        sum += arr[i];
        maxSum = max(sum, maxSum);
       
        if(sum < 0)
        {
            sum = 0;
        }
        
    }
    return maxSum;
}
